/*float 초기화*/
	.clearfix {
		min-height:1px;/*IE6, 7*/
	}
	
	.clearfix:after {
		display:block;
		clear:both;
		height:0px;
		visibility:hidden;
		content:".";
	}	
	
	* html .clearfix {
	    height: 1px;
