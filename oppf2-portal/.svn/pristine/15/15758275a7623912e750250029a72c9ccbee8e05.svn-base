/* align */
.l          {text-align:left;}
.l-imp          {text-align:left !important;}
.c          {text-align:center;}
.r          {text-align:right;}

/* float */
.float_left		{ float:left; }
.float_right	{ float:right; }
.clear_both		{ clear:both; }
.fl			{ float:left; }
.fr			{ float:right; }
.cboth	{ clear:both; }


/* display */
.hide			{ display:none; }

/*컬러*/
