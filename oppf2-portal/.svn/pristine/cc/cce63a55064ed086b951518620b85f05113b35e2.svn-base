	<!--
     * @Method Name        : apt.SptUserManageDAO.selectSptUserDeleteList
     * @Method description : 탙퇴한 일반 회원중 발급한 가상계좌가 있는 회원 목록을 조회한다.
     * @return             : SptUserManageVO
     * @throws             : Exception
     -->
	<select id="apt.SptUserManageDAO.selectSptUserDeleteList" resultClass="SptUserManageVO">
		SELECT a.customer_reg_no AS customerRegNo,
				a.customer_id AS customerId
		  FROM spt_customer_info_profile a
		 WHERE a.customer_reg_status = 'G005004'
		   AND a.customer_step = 'G006005'
       AND a.customer_withdrawal_proc_yn = 'N'
	</select>
    
