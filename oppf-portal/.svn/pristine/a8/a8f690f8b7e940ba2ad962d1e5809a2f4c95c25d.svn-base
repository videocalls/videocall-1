    -->
    <update id="usr.SvcApplDAO.updateSptCustomerServiceProfile" parameterClass="SptCustomerServiceProfileVO">
        update spt_customer_service_profile set
            terms_reg_no = #termsRegNo#,
            update_date = sysdate(),
